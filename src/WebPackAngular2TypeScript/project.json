{
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-final",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "web-dev": "Microsoft.AspNet.Server.Kestrel --config hosting.Development.json --Hosting:Environment Development",
    "web-prod": "Microsoft.AspNet.Server.Kestrel --config hosting.Production.json --Hosting:Environment Production"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "exclude": [
    "client",
    "wwwroot",
    "buildOutput",
    "typings",
    "node_modules"
  ],
  "publishExclude": [
    "typings",
    "node_modules",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}
